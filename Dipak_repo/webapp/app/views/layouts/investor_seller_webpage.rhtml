         <% end -%>



</head>

<% if @controller.controller_name != "account" %> <body><% else %><body onload="disable_forms();"<% end %>
<% if RAILS_ENV == 'production' %>
<%= clicktale_top %>
<% end %>
<div class="full">
  <div id="container" class="clearfix" >

<!-- Header-->

  	<div id="header">
              <div class="col1">
                  <div id="logo" style="*float:left;">
                      <% if @controller.controller_name != "account" %> 
                        <a href="/<%=@territory_param_name%>/<%=@business_name%>">
                      <% else %>
                        <a href="#" >
                      <% end %>
                      <% link_value =image_tag( @user_company_image.public_filename(:profile), :alt=>"thumbnail", :onload=>"resizeImage();") if (!@user_company_image.nil?) && (!@user_company_image.find_profile_thumbnail.blank?)
                        link_value = "" if link_value.blank? %>
                        <%=link_to link_value , {:controller=>"investor_seller_web_page", :action=>"seller_web_page", :permalink_text => params[:permalink_text]} %>
                       </a>
                      <%#= social_media(params[:permalink_text], "seller") %>
  	          </div>
              </div>
  	</div>

        <div class="social_media_icons">
             <%= social_media(params[:permalink_text], "seller") %>
        </div>

        <div class="seller_col2">
                  <b class="phone_size_bold"><% unless @user_company_info.nil? %>
                  <p style="float:left;"></p><%= @user_company_info.business_phone %><% end %></b><br /><br />
                  <% if @controller.controller_name != "account" %>
                      <% if params[:terms_of_use].nil? && params[:privacy_policy].nil? %>
                        <%#=link_to_remote("Contact Us",{:url => {:action => 'contact_us',:id=>@business_name, :territory_name=>@territory_param_name},
                                                                                                      :method  => :get,
                                                                                                      :update  => "lightbox_contents",
                                                                                                      :loading => "lightbox_controller.loading()",
                                                                                                      :loaded  => "lightbox_controller.loaded()"},{
                                                                                                      :title   => "Contact to Investor" }) %>
                      <% end %>
                      <% else %>
                        <a href="#" >Contact Us</a>
                  <% end %>
        </div>

      <h2 style="margin:-69px;margin-left:0px;margin-right:0px"></h2>

<!-- End Header-->
      <%= yield  %>
  </div><!--end container-->
</div>
<div style="display:none;" id="overlay_Lightbox"></div>
<div style="display:none;" id="lightbox_contents">Lightbox</div>
<div id="seller_web_footer" style="margin:auto ;text-align:center;width:960px; height:auto;padding:20px 0px;font-family:serif;">
