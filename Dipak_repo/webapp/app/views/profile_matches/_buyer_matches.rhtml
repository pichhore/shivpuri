          </div>
          <div class="ie_fix">
            <%- @boundary_overlays.each do |overlay| -%>
              <%- js = overlay.to_javascript() -%>
              <%- @map.record_init( "map.addOverlay( #{ js } );" ) -%>
            <%- end -%>
            <%- @marker_overlays.each do |overlay| -%>
              <%- js = overlay.to_javascript() -%>
              <%- @map.record_init( "map.addOverlay( #{ js } );" ) -%>
            <%- end -%>
            <%= @map.to_html(:no_load => true) if request.xhr?%>
            <%= @map.to_html({:proto_load=>true}) unless request.xhr?%>
            <%= "<script>lightbox_image_controller = new NL.LightboxImageController();</script>" if request.xhr? %>
          </div>

      <div class="clear"></div>

      <div id="profile_list" class="profile_summary">

        <div class="headings" <%="style='display:none'" if @profiles.empty? && @near_profiles.empty? %> >

          <div class="column1">Sort By:</div>
          <div class="column2">&nbsp;</div>
          <div class="column3 heading sortable <%=@sort_order if @sort_type == 'is_owner'%>" sort="is_owner">&nbsp;Seller</div>
          <div class="column4 heading sortable <%=@sort_order if @sort_type == 'price'%>" sort="price">&nbsp;Price</div>
          <div class="column5 heading sortable <%=@sort_order if @sort_type == 'privacy'%>" sort="privacy">&nbsp;Privacy</div>

          <div class="clear"></div>
        </div>

        <%=render :partial=>"/shared/profile_summary", :collection=>@profiles, :locals=>{:use_numbered_icons=>false, :exact_profiles=>@exact_profiles} %>
        <%=render :partial=>"/shared/near_profile_summary", :collection=>@near_profiles, :locals=>{:use_numbered_icons=>false} if @result_filter=='all' || @result_filter=='new' || @result_filter=='favorites' %>
        <%- if @profiles.empty? && @near_profiles.empty? -%>
        <h4>No matches found</h4>
        <%-end-%>

    </div>

    <div class="buttonbar" page_number="<%=@page_number%>" total_pages="<%=@total_pages %>">
      <div class="center">
        <%- if !@profiles.empty? || !@near_profiles.empty? -%>
        <p><%=link_to(" < Prev",{:action=>'oops'}, :class=>"prev_page") if @page_number > 1%>&nbsp;&nbsp;Page <span id="current_page_number"><%=@page_number%></span> of <span id="total_pages"><%=@total_pages %></span>&nbsp;&nbsp;
          <%=link_to(" Next > ",{:action=>'oops'}, :class=>"next_page") if @page_number < @total_pages %></p>
        <%-end-%>
      </div>
    </div>

  </fieldset>
