                <%= f.radio_button "repair_calculated_by", "my estimate" %>My Estimate <%=error_message_on :fields, :repair_calculated_by %>
            </td>
          </tr>
        <% end %>
       
         <% if (@profile.investment_type.id == 1 or @profile.investment_type.id == 3) or @profile %>
            <tr id="minmp">
                 <th id="min_mon_pay_label" <% if @fields.privacy == 'private'%> class="label" <% else %> class="label_required" <% end %>>
